nsform:translateY(0px)}13%{-webkit-transform:translateY(-3px);transform:translateY(-3px)}14%{-webkit-transform:translateY(0px);transform:translateY(0px)}}@keyframes button-bouncer{0%,100%{-webkit-transform:translateY(0px);transform:translateY(0px)}4%{-webkit-transform:translateY(-20px);transform:translateY(-20px)}8%{-webkit-transform:translateY(-25px);transform:translateY(-25px)}10%{-webkit-transform:translateY(0px);transform:translateY(0px)}11%{-webkit-transform:translateY(-8px);transform:translateY(-8px)}12%{-webkit-transform:translateY(0px);transform:translateY(0px)}13%{-webkit-transform:translateY(-3px);transform:translateY(-3px)}14%{-webkit-transform:translateY(0px);transform:translateY(0px)}}
